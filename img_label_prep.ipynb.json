  },
  {
   "cell_type": "code",
   "execution_count": 135,
   "metadata": {},
   "outputs": [],
   "source": [
    "result['label']=result['label'].apply(lambda x: np.asarray(pd.DataFrame(x).groupby(0).nth(0).reset_index()))"
